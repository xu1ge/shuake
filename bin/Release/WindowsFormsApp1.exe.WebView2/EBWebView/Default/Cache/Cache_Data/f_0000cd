$(function () {
    //Kill IE6
    var IE6 = !-[1, ] && !window.XMLHttpRequest;
    if (IE6) {
        var $IE6Code = $('#kill-ie').html(),
		    $winH = $(window).outerHeight(),
		    $body = $('body');

        $body.css({ 'position': 'relative', 'overflow': 'hidden', 'height': $winH }).append($IE6Code);
        $(window).on('resize', function () { $body.height($(window).outerHeight()); });

        return false;
    }

    var $userInfo = $('#user-info'),
	    $userDropdown = $userInfo.next();

    //个人中心
    $userInfo.on('click', function () {
        var hasClass = $(this).hasClass('current');
        hasClass ? $(this).removeClass('current') : $(this).addClass('current');
        hasClass ? $userDropdown.addClass('hidden') : $userDropdown.removeClass('hidden');
    });

    //document
    $(document).on('click', function (e) {
        var $target = $(e.target);
       
        if (!$target.parents('.navbar-user').length) {
            $userInfo.removeClass('current');
            $userDropdown.addClass('hidden');
        }
    });

    // 调整页面尺寸
    var w = window.innerWidth
	|| document.documentElement.clientWidth
	|| document.body.clientWidth;

    if (w < 1200) {
        $('.wrapper,.personal-details,.col-navbar .col-navbar-wrapper').width(960);
        $('.top-banner').css('min-width', '960px');
    }

    var resizeTimer;
    var resizeFunc = function () {
        var w = window.innerWidth
		|| document.documentElement.clientWidth
		|| document.body.clientWidth;

        if (w < 1200) { $('.wrapper,.personal-details,.col-navbar .col-navbar-wrapper').width(960); $('.top-banner').css('min-width', '960px'); }
        if (w >= 1200) { $('.wrapper,.personal-details,.col-navbar .col-navbar-wrapper').width(1200); $('.top-banner').css('min-width', '1200px'); }
    };

    $(window).resize(function (event) {
        clearTimeout(resizeTimer);
        resizeTimer = setTimeout(resizeFunc, 100);
    });
});

function handleException(result) {

    if (!result.code) {
        if (result == null || !result.__AjaxException)
            return result;

        var exc = result.__AjaxException;
        if (exc.action == "JSMethod") {
            result = null;
            eval(exc.parameter + "()");
        }
        else if (exc.action == "JSEval") {
            result = null;
            eval(exc.parameter);
        }
        else if (exc.action == "returnValue") {
            return exc.parameter;
        }
    } else {

        if (!result) {
            alert('发生未知错误，请刷新页面后尝试！');
            return false;
        }

        switch (result.code) {
            case 500:
            case 401:
            case 402:
            case 404:
            case 503:
                alert(result.msg || '发生未知错误，请刷新页面后尝试！');
                history.go(-1);
                return false;
            case 403:
                alert(result.msg || '发生未知错误，请刷新页面后尝试！');
                history.go(-1);
                return false;
        }

        return true;
    }
};(function ($) {

    $.extend({
        dynamic_dialog: function (selector) {
            if ($('#' + selector).length == 0) {
                $('body').append($('<div id="' + selector + '" />'));
            }
            return $('#' + selector);
        },
        //将querystring 转换为 json 对象
        parseParams: function(query) {
            var re = /([^&=]+)=?([^&]*)/g;
            var decodeRE = /\+/g;
            var decode = function(str) {
                return decodeURIComponent(str.replace(decodeRE, " "));
            };

            var params = {},
                e;
            while (e = re.exec(query)) {
                var k = decode(e[1]),
                    v = decode(e[2]);
                if (k.substring(k.length - 2) === '[]') {
                    k = k.substring(0, k.length - 2);
                    (params[k] || (params[k] = [])).push(v);
                } else params[k] = v;
            }
            return params;
        },
        htmlencode: function(text) {
            return $('<div />').text(text).html();
        },
        htmldecode: function(text) {
            return $('<div />').html(text).text();
        }
    });

    window.redirect = function (form, table, rowIds) {

        for (var i = 0; i < rowIds.length; i++) {
            $('tr[id=' + rowIds[i] + ']', table).remove();
        }

        $.fn.gform.working = false;

        if ($('tbody tr', table).length == 0) {

            form.find('input[name=page]').val(Math.max(parseInt($('.pagination .current', form)) - 1, 1));
            form.submit();

            return false;
        }

        form.submit();
    };

    /**
     * @name 获取地址栏中的Hash值
     * @return Object
     */
    window.getHash = function(key) {
        var arr = [],
            obj = {},
            hash = location.hash;

        if (!hash.length) return null;
        arr = hash.substring(1, hash.length).split('&');

        for (var i = 0, len = arr.length; i < len; i++) {
            var temp = arr[i].split('=');
            if (temp.length) obj[temp[0]] = temp[1];
        }

        if (key) {
            return obj[key];
        } else {
            return obj;
        }
    };

    //window.alert = function (message) {
    //    capi_alert(message);
    //};

    // window.confirm = function (message) {
    //     return capi_confirm(message);
    // }
})(jQuery);