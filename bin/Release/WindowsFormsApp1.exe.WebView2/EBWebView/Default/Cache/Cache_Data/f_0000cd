    $.fn.gform = function (opts) {
        opts = $.extend(true, {}, $.fn.gform.defaults, opts);

        return $(this).each(function (i, v) {

            var form = $(v);

            opts.url = opts.url || form.prop('action');
            opts.method = opts.method || form.prop('method');

            if (!opts.method) {

                console.error('表单的 METHOD 未定义');
                return false;
            }

            if (!opts.url) {

                console.error('表单的 ACTION 未定义');
                return false;
            }

            //绑定 form 表单
            $(v).ajaxForm({ url: opts.url, type: opts.method, beforeSerialize: opts.beforeSerialize, beforeSubmit: opts.beforeSubmit, success: opts.onSuccess });

            //执行初始化后的回调函数
            if (opts.callback && $.isFunction(opts.callback)) opts.callback.apply(this, [form]);
        });
    };

    $.fn.gform.defaults = {
        url: '',
        method: 'POST',
        beforeSerialize: function () { },
        beforeSubmit: function () { },
        onSuccess: function () { },
        callback: function () { }
    };

    $.fn.gtable = function (opts) {
        opts = $.extend(true, {}, $.fn.gtable.defaults, opts);

        return $(this).each(function (i, v) {

            var table = $(v),
                form = table.parents('form:first');

            table.addClass('x-table-container');

            //悬浮高亮
            $('tbody tr', table).hover(function () { $(this).css('background-color', '#dcf3f7'); }, function () { $(this).css('background-color', ''); });

            //绑定行选，全选，全不选            
            var checkbox = function () {

                if ($('input[type=checkbox]', table).length == 0) return false;

                //绑定 table 的行选中
                $('tbody tr', table).on('click', function () {

                    var _this = $(this),
                        checkbox = $('input[type=checkbox]', _this),
                        checked = checkbox.prop('checked');

                    checkbox.prop('checked', !checked);

                    $('thead input[type=checkbox]:first', table).prop('checked', $('tbody tr input[type=checkbox]:not(:checked)', table).length == 0);

                    table.trigger('gtable.checked', [table.get_selected_row_id()]);
                });

                //处理 checkbox 事件冒泡
                $('tbody tr input[type=checkbox]', table).on('click', function (e) {

                    $('thead input[type=checkbox]:first', table).prop('checked', $('tbody tr input[type=checkbox]:not(:checked)', table).length == 0);

                    table.trigger('gtable.checked', [table.get_selected_row_id()]);

                    e.stopPropagation();
                });

                // 全选全不选
                $('thead input[type=checkbox]:first', table).on('click', function (e) {

                    e.stopPropagation();

                    var _this = $(this),
                        checked = _this.prop('checked');

                    _this.prop('title', checked ? '全不选' : '全选');

                    $('tbody tr input[type=checkbox]', table).prop('checked', checked);

                    table.trigger('gtable.checked', [table.get_selected_row_id()]);
                });
            }();

            //绑定排序            
            var sort = function () {

                //如果未定义排序列，则不绑定排序
                if (!opts.sorts) opts.sorts = table.data('orderbys');

                //如果 table 上级没有表单，则不绑定排序
                if (!opts.sorts || form.length == 0) return false;

                //增加表单内的排序提交字段                    
                if ($('input[name=sort]', form).length == 0) form.append('<input type="hidden" name="sort" />');

                //得到排序数组                
                var sorts = opts.sorts.split(','),
                    css_arrow_all = "url(data:image/png;base64,) no-repeat right center",
                    css_arrow_up = "url(data:image/png;base64,) no-repeat right center",
                    css_arrow_down = "url(data:image/png;base64,) no-repeat right center",
                    element = $('input[name=sort]', form);

                $.each(sorts, function (i, v) {

                    var th = $('#' + v, table),
                        c_val = element.val(),
                        c_css = css_arrow_all,
                        span = $('<span></span>');

                    if (th.length == 1) {

                        if (c_val && c_val.replace('-', '') == v) {
                            c_css = c_val.indexOf('-') == 0 ? css_arrow_down : css_arrow_up;
                        }

                        th.addClass('x-table-sort').css('cursor', 'pointer').prop('title', '点击排序');

                        span.css('background', c_css).css('padding-right', '15px');
                        span.addClass(c_val.indexOf('-') == 0 ? 'x-table-sort-down' : 'x-table-sort-up');

                        th.append(span);
                    }
                });

                //绑定点击排序的事件                
                $('.x-table-sort', table).on('click', function () {
                    var _this = $(this);

                    $('.x-table-sort span', table).css('background', css_arrow_all);

                    if (_this.find('span:first').hasClass('x-table-sort-up')) {

                        element.val('-' + _this.prop('id'));
                        _this.find('span:first').removeClass('x-table-sort-up').addClass('x-table-sort-down').css('background', css_arrow_down);

                    } else {

                        element.val(_this.prop('id'));
                        _this.find('span:first').removeClass('x-table-sort-down').addClass('x-table-sort-up').css('background', css_arrow_up);
                    }

                    form.submit();
                });
            }();

            //执行初始化后的回调函数
            if (opts.callback && $.isFunction(opts.callback)) opts.callback.apply(this, [table]);
        });
    };

    $.fn.gtable.defaults = {
        sorts: '',
        callback: function () { }
    };

    //获取表格中选中的 checkbox ID
    $.fn.get_selected_row_id = function () {
        var ids = [];

        $('tbody tr td input:checkbox:checked', this).each(function (i, v) {
            ids.push(encodeURIComponent($(v).parents('tr:first').prop('id')));
        });

        return ids;
    };

})(jQuery);

