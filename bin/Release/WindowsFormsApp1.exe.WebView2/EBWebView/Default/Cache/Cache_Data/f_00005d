    <script>
      
      Views();
      resourceNum();
      
      //浏览量
      setInterval ("Views()", 5000);
      
      /*function Views(){
        jQuery.getJSON("https://tj.ahjxjy.cn:42345/?host=" + location.host + "&callback=?", function (msg) {
          $(".JS_onlineNum").text(msg.online + 1);
          $(".JS_todayNum").text(msg.today + 1);
          $(".JS_totalNum").text(msg.total + 10000000);
        });
      };*/
      
       function Views() {        
         let ajax = new XMLHttpRequest();
        ajax.open("get", `https://tj.ahjxjy.cn:42345/?host=" + ${location.host} + "&callback=?`, true);
        ajax.send();
        ajax.onreadystatechange = function () {
            if (ajax.status == 200) {
                let regex = /\((.+?)\)/g; // 小括号

                let aaa = ajax.responseText;
                let msg = ""
                if (aaa) {
                    msg = JSON.parse(aaa.match(regex)[0].replace("(", "").replace(")", ""));
                } else {
                    return

                }
                    console.log(msg);
                localStorage.setItem("msg", JSON.stringify(msg));

               
                $(".JS_onlineNum").text(msg.online + 1);
                $(".JS_todayNum").text(msg.today + 1);
                $(".JS_totalNum").text(msg.total + 10000000);
            } else {

              
                let acc = JSON.parse(localStorage.getItem("msg"));
                if (acc) {
                    $(".JS_onlineNum").text(acc.online + 1);
                    $(".JS_todayNum").text(acc.today + 1);
                    $(".JS_totalNum").text(acc.total + 10000000);
                } else {
                    $(".JS_onlineNum").text(800 + 1);
                    $(".JS_todayNum").text(30000 + 1);
                    $(".JS_totalNum").text(451381739 + 10000000);
                }
            }
        };


                               };
      
      //资源总数
      function resourceNum() {        
        jQuery.getJSON("https://main.ahjxjy.cn/api/External/getOverview",function(data) {
          
          $(".JS_resCount").text(data.resCount);
          $(".JS_videoCount").text(data.videoCount);
          $(".JS_courseCount").text(data.courseCount);
          $(".JS_sharedCourseCount").text(data.sharedCourseCount);
          $(".JS_majorCount").text(data.majorCount);
        });
      };
    </script>
    <script>
      //学位外语
      //var btn = document.getElementsByClassName("serverClick")[2];
      function threeFn(){
        debugger;
        jQuery.post('https://main.ahjxjy.cn/portal/ajax/supermarket/degreeenglishsso', function (r) {
          if (r.code > 0 ) {
            location.href = "http://sso.ahjxjy.cn/login.html?ReturnUrl=http://sso.ahjxjy.cn/ssoclient.html&" + r.msg;
          }else{
            alert(r.msg);
            return false;
          }
        });
        return true;
      };
    </script>
    <script>
      //课程超市课程类
      setTimeout(function () {    
        jQuery(".project_tab .pro_title").slide({mainCell:".keep",effect:"left",autoPlay:false,pnLoop:false});
      }, 1000);
      
    </script>
    
